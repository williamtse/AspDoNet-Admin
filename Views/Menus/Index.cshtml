@model MvcMovie.Models.Menu

@{
    ViewData["Title"] = "Index";
    var menuList = ViewData["menuList"];
    var form = ViewData["form"];
    var script = ViewData["script"];
}

<div class="row">
    <div class="col-sm-6">
        @Html.Raw(menuList)
    </div>

    <div class="col-sm-6">
        @Html.Raw(form)
    </div>
</div>


<script>
    $(function () {
        $('#tree').nestable([]);

        @Html.Raw(script)
    })
</script>